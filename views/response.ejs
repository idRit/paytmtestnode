<html>

<head>
    <title>Merchant Check Out Page</title>
</head>

<body>
    <div class="receipt">
        <!--    <% if (resultData == 'true') { %>
    <center><h1>validation succesfull</h1></center>
    <% }else
    { %>
    <center><h1>validation unsuccesfull</h1></center>
    <% } %>-->
        <table class="table-receipt">
            <thead>
                <tr>
                    <th class="th-header" colspan="2">PAYMENT RECEIPT
                    </th>
                </tr>
            </thead>
            <tbody>
                <% for (name in responseData) {
        %>
                <% if(name !== "CHECKSUMHASH"){ %>
                <tr>
                    <td class="td-title"><%= name %></td>
                    <td class="td-content"><%= responseData[name] %></td>
                </tr>
                <% } %>
                <% } %>
                <tr>
                    <td class="td-bottom" colspan="2">Thank you!.
                    </td>
                </tr>
            </tbody>
        </table>
    </div>


    <button onclick="done()">done</button>

    <script>
        function done() {
            let url = location.protocol + '//' + location.hostname + (location.port ? ':' + location.port : '');
            window.location = url;
        }
    </script>
</body>

</html>